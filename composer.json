    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/App"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "php": ">=7.4",
        "madpeterz/yapfbootstrap": "^1.0",
        "phpoffice/phpspreadsheet": "^1.18"
    },
    "authors": [
